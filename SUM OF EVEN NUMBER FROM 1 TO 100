/* JAVA PROGRAM TO  PRINT SUM OF EVEN NUMBER FROM 1 TO 100 */

import java.util.scanner;

public class Main {
    public static void main(String[] args) {
      int sum=0;
      for(int i=sum;i<=100;i++){
        if(i%2==0)
          sum=sum+i;
         
      }
      System.out.println(sum);
  }
}
